<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="280.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.DeleteController">
    <children>
        <Label layoutX="41.0" layoutY="20.0" prefHeight="44.0" prefWidth="149.0" text="Nhập từ cần xóa" textFill="#1e2956">
            <font>
                <Font name="Quicksand SemiBold" size="18.0" />
            </font>
        </Label>
        <TextArea fx:id="word" layoutX="41.0" layoutY="66.0" prefHeight="160.0" prefWidth="318.0" />
        <Button fx:id="deleteButton" layoutX="393.0" layoutY="92.0" mnemonicParsing="false" onAction="#deleteButtonClicked" prefHeight="38.0" prefWidth="78.0" text="Hoàn tất" textFill="#1e2956">
         <font>
            <Font name="Quicksand SemiBold" size="12.0" />
         </font></Button>
        <Button fx:id="closeButton" layoutX="393.0" layoutY="146.0" mnemonicParsing="false" onAction="#closeWindow" prefHeight="38.0" prefWidth="78.0" text="Thoát" textFill="#fec400">
         <font>
            <Font name="Quicksand SemiBold" size="12.0" />
         </font></Button>
    </children>
</AnchorPane>
